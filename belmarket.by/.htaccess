# There might be no access to apache config, set options here
DirectoryIndex index2.php index.html
Options -Indexes FollowSymLinks -MultiViews

#SSLVerifyClient require
#SSLRequire %{SSL_CLIENT_S_DN_OU} eq "Admin" or %{SSL_CLIENT_S_DN_OU} eq "belmarket"

ExpiresActive On 
ExpiresByType text/html "access plus 10 minutes"
ExpiresByType text/xml "access plus 10 minutes"
ExpiresByType application/rss+xml "access plus 10 minutes"
ExpiresDefault "access plus 30 days"
AddOutputFilterByType DEFLATE text/plain text/xml application/javascript text/css

# Options might not aloowed here, use Rewrite rules instead
<IfModule mod_rewrite.c>
    RewriteEngine On
    
    RewriteCond %{REQUEST_URI} ^/+get_img$
    RewriteRule . /get_img2.php [L]
    
    RewriteCond %{REQUEST_URI} ^/+attachment/+
    RewriteRule . /attachment.php [L]

    RewriteCond %{REQUEST_URI} !\.php$    
    RewriteCond %{REQUEST_URI} !\.ico$
    RewriteCond %{REQUEST_URI} !\.gz$
    RewriteCond %{REQUEST_URI} !\.cnf$
    RewriteCond %{REQUEST_URI} !\.html$
    RewriteCond %{REQUEST_URI} !\.css$
    RewriteCond %{REQUEST_URI} !\.js$
    RewriteCond %{REQUEST_URI} !\.rss$
    RewriteCond %{REQUEST_URI} !\.xml$
    RewriteCond %{REQUEST_URI} !^/+admin
    RewriteCond %{REQUEST_URI} !^/+feed
    RewriteCond %{REQUEST_URI} !^(/+plugins/[^/]*)?/+js
    RewriteCond %{REQUEST_URI} !^/sys-templates
    RewriteCond %{REQUEST_URI} !^(/+plugins/[^/]*)?/+css
    RewriteCond %{REQUEST_URI} !^(/+plugins/[^/]*)?/+images
    RewriteCond %{REQUEST_URI} !^/+templates/+
    RewriteRule . /index2.php [L]
    
    RewriteCond %{REQUEST_URI} .tpl$
    RewriteRule . /index2.php [L]
    
    
    #RewriteCond %{REQUEST_URI} ^/+admin$|^/+admin/+.*|^/+admin-files
    #RewriteCond %{SERVER_PORT}  ^80
    #RewriteRule ^(.*)$ https://www.belmarket.by/$1 [L]
    
    RewriteCond %{REQUEST_URI} ^/+admin$|^/+admin/+.*|^/+admin-files
    RewriteRule . /admin2.php [L]
    
</IfModule>
